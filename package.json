{
  "name": "hatenablogs",
  "version": "1.0.0",
  "description": "Neo's Hatena Blogs",
  "private": true,
  "scripts": {
    "gulp": "gulp",
    "strip-bom": "strip-bom",
    "css": "gulp build-css && npm run css-strip",
    "css-corredor": "gulp build-css-corredor && npm run css-strip-corredor",
    "css-murga": "gulp build-css-murga && npm run css-strip-murga",
    "css-elmylar": "gulp build-css-elmylar && npm run css-strip-elmylar",
    "css-bitarcher": "gulp build-css-bitarcher",
    "css-strip": "npm run css-strip-corredor && npm run css-strip-murga && npm run css-strip-elmylar",
    "css-strip-corredor": "mv ./dist/styles/Corredor.css ./dist/styles/Corredor-old.css && strip-bom ./dist/styles/Corredor-old.css > ./dist/styles/Corredor.css && rm ./dist/styles/Corredor-old.css",
    "css-strip-murga": "mv ./dist/styles/Murga.css ./dist/styles/Murga-old.css && strip-bom ./dist/styles/Murga-old.css > ./dist/styles/Murga.css && rm ./dist/styles/Murga-old.css",
    "css-strip-elmylar": "mv ./dist/styles/ElMylar.css ./dist/styles/ElMylar-old.css && strip-bom ./dist/styles/ElMylar-old.css > ./dist/styles/ElMylar.css && rm ./dist/styles/ElMylar-old.css",
    "js": "gulp build-js",
    "js-corredor": "gulp build-js-corredor",
    "js-murga": "gulp build-js-murga",
    "js-elmylar": "gulp build-js-elmylar",
    "js-bitarcher": "gulp build-js-bitarcher"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neos21/HatenaBlogs.git"
  },
  "author": "Neo <neos21@gmail.com> (http://neo.s21.xrea.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Neos21/HatenaBlogs/issues"
  },
  "homepage": "https://github.com/Neos21/HatenaBlogs#readme",
  "devDependencies": {
    "@neos21/neos-normalize": "1.0.3",
    "gulp": "3.9.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-plumber": "1.1.0",
    "gulp-sass": "3.1.0",
    "gulp-uglify": "2.1.2",
    "strip-bom-cli": "1.1.0"
  }
}
